{
  "schemaVersion": 1,
  "id": "bettercommandline",
  "version": "${version}",
  "name": "BetterCommandLine",
  "description": "A mod that adds a better command line to Minecraft.\nYou can use UP and DOWN arrow keys to navigate through the command history.",
  "authors": [
    "crl"
  ],
  "contact": {
    "repo": "https://github.com/crlcrl1/BetterCommandLine"
  },
  "license": "MIT",
  "environment": "*",
  "icon": "assets/bettercommandline/icon.png",
  "entrypoints": {
    "client": [
      "com.crl.bettercommandline.client.BetterCommandLineClient"
    ],
    "main": [
      "com.crl.bettercommandline.BetterCommandLine"
    ],
    "modmenu": [
      "com.crl.bettercommandline.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "bettercommandline.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "modmenu": "*"
  }
}
